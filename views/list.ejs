    <%- include("header") -%>
    <div class="box" id="heading">
        <h1>
            <%= listTitle %>
        </h1>
    </div>
    <div class="box">
        <% for(i=0;i<items.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= items[i] %>
                </p>
            </div>
            <% } %>
            <form action="/" method="POST">
                <input type="text" name="newItem" placeholder="Enter New Item" autocomplete="off">
                <button type="submit" name="list" value= <%= listTitle %> >+</button>
            </form>
    </div>
    <%- include("footer") -%>
